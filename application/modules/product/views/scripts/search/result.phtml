       $('#compare').click(function(e){ 
            var val = []; 
            var selected = new Array();
            $(':checkbox:checked').each(function(i){
                val[i] = $(this).attr('id');
                selected.push(val[i]);
            });
            var count = selected.length;
            if(count == '0'){
                $(".banner-alert").attr('style','display:none;');
                alert('Plesae select atleast one product');
                return false;
            }else{ 
                $.fancybox.showLoading();
                 $.ajax({
                    type : "POST",
                    cache : false,
                    url : "/product/Comparison/compare",
                    data  : 'productId=' + selected,
                    success : function(selected) {
                      $.fancybox(selected);
                    }
                  });
                  return false;
            }//end else
